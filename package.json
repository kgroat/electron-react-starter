{
  "name": "electron-test-app",
  "identifier": "com.github.electrontestapp",
  "appName": "App Name",
  "icon": "appicon.png",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "platforms": {
    "darwin": "3.0.13",
    "linux32": "3.0.13",
    "linux64": "3.0.13",
    "win32": "3.0.13",
    "win64": "3.0.13"
  },
  "scripts": {
    "test": "run-s -c test:lint test:unit",
    "test:lint": "tslint --config .config/tslint.json --project .config/tsconfig.json",
    "test:unit": "jest --config=.config/jest.json",
    "start": "run-p --race scss:types:watch start:server start:app",
    "start:server": "webpack-dev-server --config .config/webpack.dist.js",
    "start:app": "run-s start:app:build start:app:start",
    "start:app:build": "webpack --config .config/webpack.app.dev.js",
    "start:app:start": "sleepms 5000 && electron intermediates/app/index.js",
    "build": "NODE_ENV=production run-s build:archives-dir build:icons build:bundle build:pack build:outputs",
    "build:archives-dir": "mkdir -p intermediates/archives",
    "build:icons": "electron-icon-maker --input=./appicon.png --output=./intermediates",
    "build:bundle": "webpack --config .config/webpack.config.js",
    "build:pack": "asar pack intermediates/dist intermediates/bundle.asar",
    "build:outputs": "node ./scripts/generateOutputs/index.js",
    "storybook": "run-p scss:types:watch storybook:server storybook:open",
    "storybook:server": "start-storybook -p 6006",
    "storybook:open": "sleepms 5000 && opn http://localhost:6006/",
    "build-storybook": "run-s scss:types build-storybook:build",
    "build-storybook:build": "build-storybook",
    "scss:types": "tcm -p ./src/**/**/**/*.scss",
    "scss:types:watch": "npm run scss:types -- -w"
  },
  "private": true,
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^4.1.2",
    "@types/deep-freeze": "^0.1.1",
    "@types/enzyme": "^2.8.7",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.28",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-test-renderer": "^15.5.4",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.3",
    "@types/redux-persist": "^4.3.1",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/uuid": "^3.4.2",
    "@types/webpack-env": "^1.13.1",
    "asar": "^0.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.0",
    "css-loader": "^2.0.1",
    "deep-freeze": "0.0.1",
    "electron": "^3.0.13",
    "electron-icon-maker": "0.0.4",
    "electron-log": "^2.2.17",
    "electron-redux": "^1.3.1",
    "enzyme": "^2.9.1",
    "extract-zip": "^1.6.7",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "json-loader": "^0.5.7",
    "mdi": "^2.0.46",
    "ncp": "^2.0.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.2",
    "opn-cli": "^3.1.0",
    "plist": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "rcedit": "^0.9.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^6.0.0",
    "react-test-renderer": "^15.6.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.9.1",
    "redux-thunk": "^2.2.0",
    "request": "^2.88.0",
    "resolve-url-loader": "^2.3.1",
    "rimraf": "^2.6.2",
    "roboto-fontface": "^0.8.0",
    "rxjs": "^5.4.3",
    "sass-loader": "^7.1.0",
    "sleep-ms": "^2.0.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^6.0.1",
    "typed-css-modules": "^0.3.7",
    "typescript": "^3.2.2",
    "unzip-stream": "^0.1.2",
    "unzipper": "^0.9.6",
    "uuid": "^3.1.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
