{
  "name": "electron-test-app",
  "identifier": "com.github.electrontestapp",
  "appName": "App Name",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "scripts/parallelRace.js scss:types:watch start:server start:electron",
    "start:server": "webpack-dev-server --config .config/webpack.config.js",
    "start:electron": "sleepms 5000 && electron app/dev.js",
    "build": "NODE_ENV=prod run-s intermediates bundle package generateOutputs",
    "test": "run-s test:lint test:unit",
    "test:lint": "tslint --config .config/tslint.json --project .config/tsconfig.json",
    "test:unit": "jest --config=.config/jest.json",
    "storybook": "concurrently 'npm run scss:types:watch' 'start-storybook -p 6006' 'sleepms 5000 && opn http://localhost:6006/'",
    "build-storybook": "build-storybook",
    "scss:types": "tcm -p ./src/**/**/**/*.scss",
    "scss:types:watch": "npm run scss:types -- -w",
    "intermediates": "mkdir -p intermediates/archives",
    "bundle": "webpack --config .config/webpack.config.js",
    "package": "asar pack intermediates/dist intermediates/bundle.asar",
    "generateOutputs": "node ./scripts/generateOutputs/index.js"
  },
  "private": true,
  "author": "",
  "license": "ISC",
